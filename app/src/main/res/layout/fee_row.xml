<android.support.v7.widget.CardView
    android:id="@+id/cardViewfee"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"

    app:cardElevation="16dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!--app:cardCornerRadius="@dimen/activity_vertical_margin"-->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >


        <!--<TextView-->
            <!--android:id="@+id/title"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_alignParentTop="true"-->
            <!--android:layout_marginTop="1dp"-->
            <!--android:layout_marginLeft="10dp"-->
            <!--android:layout_centerInParent="true"-->
            <!--android:layout_toRightOf="@+id/circleView"-->
            <!--android:text="Title"-->
            <!--android:textSize="22dp" />-->

        <!--<TextView-->
            <!--android:id="@+id/description"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->


            <!--android:layout_marginLeft="10dp"-->
            <!--android:layout_centerInParent="true"-->
            <!--android:layout_below="@+id/title"-->
            <!--android:text="Description"-->
            <!--android:textSize="12dp" />-->

        <Button
            android:id="@+id/lbs"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"

            android:background="@color/colorPrimary"

            android:onClick="onClick"
            android:paddingRight="10dp"
            android:text="Fee"
            android:tag="down"

            android:drawableRight="@android:drawable/arrow_down_float"
            android:textColor="#FFF" />

        <com.github.aakira.expandablelayout.ExpandableRelativeLayout
            android:id="@+id/expandableLayout"
            android:layout_below="@+id/lbs"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:ael_expanded="false"
            app:ael_duration="500"
            app:ael_interpolator="bounce"
            app:ael_orientation="vertical">

            <TextView
                android:id="@+id/recieved"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="Recieved" />
            <TextView
                android:id="@+id/remaining"
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/recieved"
                android:text="Remaining" />

            <TextView
                android:id="@+id/advance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/remaining"
                android:layout_margin="5dp"
                android:text="Advance" />

            <TextView
                android:id="@+id/issuedate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/advance"
                android:layout_margin="5dp"
                android:text="Issue Date" />

            <TextView
                android:id="@+id/expiredate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/issuedate"
                android:layout_margin="5dp"
                android:text="Expire Date" />

            <TextView
                android:id="@+id/submittedAt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/expiredate"
                android:layout_margin="5dp"
                android:text="Expire Date" />

            <TextView
                android:id="@+id/arrears"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/submittedAt"
                android:layout_margin="5dp"
                android:text="Expire Date" />
            <TextView
                android:id="@+id/fee"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/arrears"
                android:layout_margin="5dp"
                android:text="Expire Date" />

        </com.github.aakira.expandablelayout.ExpandableRelativeLayout>



    </RelativeLayout>


</android.support.v7.widget.CardView>